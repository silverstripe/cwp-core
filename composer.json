{
    "name": "cwp/cwp-core",
    "description": "CWP basic compatibility module. Use for migrations.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2",
        "silverstripe/hybridsessions": "^3",
        "silverstripe/environmentcheck": "^3",
        "silverstripe/auditor": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "CWP\\Core\\": "src/",
            "CWP\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "css",
            "javascript"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}